<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">


   <context:annotation-config/>
   
   <!--bean class = "com.tutorialspoint.InitPostProcessor" />  -->
   
   <bean id = "cStartEventHandler" class = "com.tutorialspoint.CStartEventHandler"/>
   
   <bean id = "customEventHandler" class = "com.tutorialspoint.HelloCustomEventHandler"/>
   <bean id = "customEventPublisher" class = "com.tutorialspoint.HelloCustomEventPublisher"/>
   
   <bean id = "helloWorld" class = "com.tutorialspoint.HelloWorld" scope = "prototype">
      <property name = "message" value = "Hello Spring World!"/>
   </bean>
   
   <bean id = "carBean" class = "com.tutorialspoint.Car">
      <property name = "model" value = "Generic Car Model"/>
   </bean>
   
   
   <!-- Definition for textEditor bean -->
   <bean id = "textEditor" class = "com.textEditor.TextEditor" autowire = "byType">
   </bean>

   <!-- Definition for spellChecker bean -->
   <bean id = "spellChecker" class = "com.textEditor.SpellChecker"></bean>

</beans>